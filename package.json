{
	"name": "hypercode",
	"displayName": "HyperCode",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "1.54.3",
		"node": "12.18.3"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hypercode.start",
		"onCommand:hypercode.join"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hypercode.start",
				"title": "Start a new HyperCode session"
			},
			{
				"command": "hypercode.join",
				"title": "Join an existing HyperCode session"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.51.1",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"electron-rebuild": "^2.3.5",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"node-abi": "^2.21.0",
		"typescript": "^4.2.3",
		"vscode-test": "^1.4.0",
		"prebuild": "^10.0.1"
	},
	"dependencies": {
		"@seandawson/hypermerge": "^2.0.10-ad"
	},
	"resolutions": {
		"sodium-native": "3.2.1"
	}
}
